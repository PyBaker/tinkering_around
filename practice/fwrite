#include <stdio.h>
#include <unistd.h>
/**
 * main - main function
 * Description: to test write function
 *
 * Return: 1
 */
int main(void)
{
char greet[] = "Hey Dave";
fwrite(greet, 1, sizeof(greet), stdout);
/*
 * strcpy(greet,"Hello Fred");
 *
 * 9 = write(1, "Hey Dave", 9);
 */
printf("\n\nsize of this is %lu", sizeof(greet));
return (1);
}
